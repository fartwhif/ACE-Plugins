	<Target Name="PostBuild" AfterTargets="PostBuildEvent">
		<Exec Command="set src=$(TargetDir)&#xD;&#xA;copy &quot;$(ProjectDir)web.js&quot; &quot;%25src%25web.js&quot;&#xD;&#xA;&#xD;&#xA;rem these are not needed as they are included with ACE&#xD;&#xA;del /f /q &quot;%25src%25\ACE.Server.*&quot;&#xD;&#xA;del /f /q &quot;%25src%25\ACE.Database.*&quot;&#xD;&#xA;del /f /q &quot;%25src%25\ACE.DatLoader.*&quot;&#xD;&#xA;del /f /q &quot;%25src%25\ACE.Entity.*&quot;&#xD;&#xA;del /f /q &quot;%25src%25\ACE.Common.*&quot;&#xD;&#xA;del /f /q &quot;%25src%25\Microsoft.Extensions.DependencyModel.dll&quot;&#xD;&#xA;&#xD;&#xA;rem plugins this plugin uses are required to also be installed and enabled&#xD;&#xA;del /f /q &quot;%25src%25\ACE.Plugin.Crypto.*&quot;&#xD;&#xA;&#xD;&#xA;rem change the dst folder to the main ACE folder to copy needed files automatically&#xD;&#xA;set dst=$(SolutionDir)..\Dependencies\ACE\&#xD;&#xA;set subdst=Plugins\ACE.Plugin.Web&#xD;&#xA;IF EXIST %25dst%25 (&#xD;&#xA;robocopy &quot;%25src%25 &quot; &quot;%25dst%25%25subdst%25 &quot; &quot;*.*&quot; /XO /NJH /NP /S&#xD;&#xA;SET ERRORLEVEL = 0&#xD;&#xA;)&#xD;&#xA;rem run custom post-build script&#xD;&#xA;set cust=$(SolutionDir)..\Dependencies\custom-postbuild.bat&#xD;&#xA;IF EXIST %25cust%25 (&#xD;&#xA;call %25cust%25 $(SolutionDir)&#xD;&#xA;SET ERRORLEVEL = 0&#xD;&#xA;)&#xD;&#xA;" />
